<%= simple_form_for(trip) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.input :starting_date, as: :date, html5: true %>
  <%= f.input :ending_date, as: :date, html5: true %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
